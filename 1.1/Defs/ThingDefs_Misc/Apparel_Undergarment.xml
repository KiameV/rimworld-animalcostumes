<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="ApparelMakeableBase">
		<defName>Apparel_AnimalCostumeUndergarment</defName>
		<label>Undergarment</label>
		<description>An undergarment for wearing under an imbued animal costume.</description>
		<graphicData>
			<texPath>Apparel/AnimalCostumes/Undergarment/AnimalCostumeUndergarment</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
			<researchPrerequisite>AnimalCostumes</researchPrerequisite>
			<skillRequirements>
				<Crafting>8</Crafting>
			</skillRequirements>
		</recipeMaker>
		<techLevel>Industrial</techLevel>
		<costStuffCount>10</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<thingCategories>
			<li>Apparel</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>7500</WorkToMake>
			<Mass>0.01</Mass>
			<Flammability>0.6</Flammability>
			<ArmorRating_Blunt>0.1</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.1</ArmorRating_Sharp>
			<ArmorRating_Heat>0.25</ArmorRating_Heat>
			<StuffEffectMultiplierArmor>0.25</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.1</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.1</StuffEffectMultiplierInsulation_Heat>
		</statBases>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Legs</li>
				<li>Arms</li>
			</bodyPartGroups>
			<wornGraphicPath>Apparel/AnimalCostumes/Undergarment/AnimalCostumeUndergarment</wornGraphicPath>
			<layers>
				<li>OnSkin</li>
			</layers>
			<tags>
				<li>Tribal</li>
				<li>Outlander</li>
				<li>TribalTrader</li>
				<li>OutlanderTrader</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<generateCommonality>0.5</generateCommonality>
	</ThingDef>
	
