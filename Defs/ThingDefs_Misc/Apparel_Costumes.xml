<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<AnimalCostumes.AnimalCostumeDef ParentName="ArmorMachineableBase">
		<defName>Apparel_AlpacaCostume</defName>
		<label>Alpaca Costume</label>
		<description>A fursuit capable of producing wool which will automatically be sheared when it gets to be too long.</description>
		<graphicData>
			<texPath>Apparel/AnimalCostumes/Alpaca/Body</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<techLevel>Industrial</techLevel>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
			<researchPrerequisite>AnimalCostumes</researchPrerequisite>
			<skillRequirements>
				<Crafting>8</Crafting>
			</skillRequirements>
		</recipeMaker>
		<stuffCategories>
			<li>Leathery</li>
		</stuffCategories>
		<thingCategories>
			<li>Apparel</li>
		</thingCategories>
		<costStuffCount>250</costStuffCount>
		<costList>
			<ComponentIndustrial>4</ComponentIndustrial>
			<Steel>75</Steel>
			<Plasteel>15</Plasteel>
		</costList>
		<statBases>
			<WorkToMake>35000</WorkToMake>
			<MaxHitPoints>300</MaxHitPoints>
			<Mass>5</Mass>
			<Flammability>0.35</Flammability>
			<ArmorRating_Sharp>0.3</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.3</ArmorRating_Blunt>
			<StuffEffectMultiplierArmor>0.25</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.1</StuffEffectMultiplierInsulation_Cold>
			<Insulation_Cold>20</Insulation_Cold>
			<EquipDelay>4</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<TameAnimalChance>0.1</TameAnimalChance>
			<TrainAnimalChance>0.1</TrainAnimalChance>
			<ToxicSensitivity>-0.1</ToxicSensitivity>
			<BedRestEffectiveness>0.1</BedRestEffectiveness>
			<MentalBreakThreshold>-0.15</MentalBreakThreshold>
			<PsychicSensitivity>-0.15</PsychicSensitivity>
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
				<li>Neck</li>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<wornGraphicPath>Apparel/AnimalCostumes/Alpaca/Body</wornGraphicPath>
			<layers>
				<li>Shell</li>
				<li>Middle</li>
				<li>OnSkin</li>
			</layers>
			<tags>
				<li>Tribal</li>
				<li>Outlander</li>
				<li>TribalTrader</li>
				<li>OutlanderTrader</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
		</apparel>
		<comps>
			<li Class="AnimalCostumes.CompProperties_Shearable_AC">
				<thingDef>SheepWool</thingDef>
				<intervalDays>10</intervalDays>
				<amount>30</amount>
			</li>
			<li Class="HediffApparel.CompProperties_HediffApparel">
				<hediffDef>AnimalCostumes_DexterousHoovesHand</hediffDef>
				<partsToAffect>
					<li>Hand</li>
				</partsToAffect>
			</li>
			<li Class="HediffApparel.CompProperties_HediffApparel">
				<hediffDef>AnimalCostumes_HoovedFeet</hediffDef>
				<partsToAffect>
					<li>Foot</li>
				</partsToAffect>
			</li>
		</comps>
		<generateCommonality>2</generateCommonality>
		<CostumeBreed>Alpaca</CostumeBreed>
		<CostumeType>Body</CostumeType>
		<thingClass>AnimalCostumes.AnimalCostume</thingClass>
	</AnimalCostumes.AnimalCostumeDef>

