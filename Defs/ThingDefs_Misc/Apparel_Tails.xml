<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<AnimalCostumes.AnimalCostumeDef ParentName="ApparelMakeableBase">
		<defName>Apparel_TanookiTail</defName>
		<label>Tanooki Tail</label>
		<description>A tanooki tail.</description>
		<graphicData>
			<texPath>Apparel/AnimalCostumes/Tanooki/Tail</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
			<researchPrerequisite>AnimalTailsAndEars</researchPrerequisite>
			<skillRequirements>
				<Crafting>6</Crafting>
			</skillRequirements>
		</recipeMaker>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<thingCategories>
			<li>Apparel</li>
		</thingCategories>
		<costStuffCount>85</costStuffCount>
		<costList>
			<Steel>10</Steel>
		</costList>
		<statBases>
			<WorkToMake>7500</WorkToMake>
			<MaxHitPoints>200</MaxHitPoints>
			<Mass>2</Mass>
			<Flammability>0.6</Flammability>
			<ArmorRating_Sharp>0.1</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.1</ArmorRating_Blunt>
			<ArmorRating_Heat>0.15</ArmorRating_Heat>
			<StuffEffectMultiplierArmor>0.25</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.05</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.05</StuffEffectMultiplierInsulation_Heat>
			<Insulation_Cold>0.5</Insulation_Cold>
			<Insulation_Heat>0.5</Insulation_Heat>
			<EquipDelay>4</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>0.05</MoveSpeed>
			<TameAnimalChance>0.01</TameAnimalChance>
			<TrainAnimalChance>0.01</TrainAnimalChance>
			<ToxicSensitivity>-0.01</ToxicSensitivity>
			<ShootingAccuracyPawn>-0.01</ShootingAccuracyPawn>
			<MeleeHitChance>-0.01</MeleeHitChance>
			<SocialImpact>0.01</SocialImpact>
			<NegotiationAbility>0.01</NegotiationAbility>
			<MedicalTendSpeed>0.02</MedicalTendSpeed>
			<ConstructionSpeed>0.01</ConstructionSpeed>
			<SmithingSpeed>0.01</SmithingSpeed>
			<SmeltingSpeed>0.01</SmeltingSpeed>
			<ButcheryMechanoidSpeed>0.01</ButcheryMechanoidSpeed>
			<SmoothingSpeed>0.01</SmoothingSpeed>
			<EatingSpeed>0.01</EatingSpeed>
			<BedRestEffectiveness>0.01</BedRestEffectiveness>
			<WorkSpeedGlobal>0.01</WorkSpeedGlobal>
			<ResearchSpeed>0.01</ResearchSpeed>
			<WorkTableWorkSpeedFactor>0.01</WorkTableWorkSpeedFactor>
			<MiningSpeed>0.01</MiningSpeed>
			<MentalBreakThreshold>-0.1</MentalBreakThreshold>
			<PsychicSensitivity>-0.1</PsychicSensitivity>
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
			</bodyPartGroups>
			<wornGraphicPath>Apparel/AnimalCostumes/Tanooki/Tail</wornGraphicPath>
			<layers>
				<li>Middle</li>
			</layers>
			<tags>
				<li>Tribal</li>
				<li>Outlander</li>
				<li>TribalTrader</li>
				<li>OutlanderTrader</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
		</apparel>
		<generateCommonality>2</generateCommonality>
		<CostumeBreed>Tanooki</CostumeBreed>
		<CostumeType>Tail</CostumeType>
		<thingClass>AnimalCostumes.AnimalCostume</thingClass>
	</AnimalCostumes.AnimalCostumeDef>

